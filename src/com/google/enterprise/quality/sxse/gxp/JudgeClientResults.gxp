<gxp:template name="com.google.enterprise.quality.sxse.gxp.JudgeClientResults"
              xmlns="http://www.w3.org/1999/xhtml"
              xmlns:call="http://google.com/2001/gxp/call"
              xmlns:expr="http://google.com/2001/gxp/expressions"
              xmlns:gxp="http://google.com/2001/gxp">

<gxp:param name='allowJudgment' type='Boolean' />
<gxp:param name='firstPolicyUri' type='String' />
<gxp:param name='secondPolicyUri' type='String' />

<call:JudgeButtons expr:disableJudgment='!allowJudgment' />

<div id="sxs">
  <call:ClientResults isFirstPolicy='true' expr:policyUri='firstPolicyUri' />
  <call:ClientResults isFirstPolicy='false' expr:policyUri='secondPolicyUri' />
</div>

<call:JudgeButtons expr:disableJudgment='!allowJudgment' />

</gxp:template>
